  "description": "Muon.",
  "repository": {
    "type": "git",
    "url": "https://github.com/petros-ioannou1/yapp"
  },
  "dependencies": {
    "easy": "^11.1.22",
    "easy-with-style": "^3.0.30",
    "necessary": "^7.1.10"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/preset-env": "^7.11.0",
    "browserify": "^17.0.0",
    "express": "^4.17.1",
    "juxtapose": "^3.0.22",
    "lively-cli": "^2.0.7",
    "watchful-cli": "^1.6.15"
  },
  "scripts": {
    "start": "node ./bin/main.js",
    "clean": "rmdir /Q /S lib",
    "watchful": "watchful -mp=4 --transpiler=babel --bundler=browserify --source-directory=./src --lib-directory=./lib --entry-file=example.js --bundle-file=./example.js --pause=100",
    "batch": "npm run watchful batch --",
    "batch-debug": "npm run watchful batch -- --debug",
    "incremental": "npm run watchful incremental --",
    "incremental-debug": "npm run watchful incremental -- --debug",
    "build": "npm run clean && npm run batch",
    "build-debug": "npm run clean && npm run batch-debug",
    "watch": "npm run clean && npm run batch && npm run incremental",
    "watch-debug": "npm run clean && npm run batch-debug && npm run incremental-debug"
  },
  "main": "./lib/index.js"
}
